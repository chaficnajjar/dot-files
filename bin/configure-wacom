#!/bin/bash

xsetwacom --set "Wacom Intuos Pro M Pad pad" Button 2 "key +ctrl +shift z -ctrl -shift"
xsetwacom --set "Wacom Intuos Pro M Pad pad" Button 3 "key +ctrl z -ctrl"
